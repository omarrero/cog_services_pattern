{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "baseName": {
        "value": "olivio"
      },
      "environmentName": {
        "value": "dev"
      },
      "defaultConsistencyLevel": {
        "value": "Session"
      },
      "maxStalenessPrefix": {
        "value": 100000
      },
      "maxIntervalInSeconds": {
        "value": 300
      },
      "systemManagedFailover": {
        "value": true
      },

      "location": {
        "value": "eastus2"
      },
      "secondaryRegion": {
        "value": "eastus"
      },
      "databaseName": {
        "value": "myDatabase"
      },
      "containerName": {
        "value": "myContainer"
      },
      "throughput": {
        "value": 400
      }
    }
  }
